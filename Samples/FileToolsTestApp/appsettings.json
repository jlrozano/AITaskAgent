{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System.Net.Http": "Warning",
            "AITaskAgent": "Information",
            "AITaskAgent.LLM": "Trace",
            "AITaskAgent.Core.Steps": "None",
            "AITaskAgent.FileTools": "Trace",
            "OpenAILLmService": "Warning"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "AITaskAgent": "Information",
                "AITaskAgent.LLM": "Verbose",
                "AITaskAgent.Core.Steps": "Fatal",
                "AITaskAgent.FileTools": "Verbose"
            }
        }
    },
    "AITaskAgent": {
        "LlmProviders": {
            "Providers": {
                "OpenRouter": {
                    "Provider": "OpenAI",
                    "BaseUrl": "https://openrouter.ai/api/v1",
                    "ApiKey": "<key>",
                    "Model": "openai/gpt-oss-120b:free",
                    "Temperature": 0.7,
                    "MaxTokens": 32000
                }
            },
            "DefaultProvider": "OpenRouter"
        }
    }
}