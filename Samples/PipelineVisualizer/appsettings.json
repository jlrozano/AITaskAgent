{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System.Net.Http": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "AITaskAgent": {
        "TemplatesPath": "templates",
        "LlmProviders": {
            "Providers": {
              "IntentionAnalyzer": {
                "Provider": "Google",
                //"BaseUrl": "https://generativelanguage.googleapis.com/v1beta/openai/",
                //"ApiKey": "<key>",
                //"Model": "gemini-2.5-flash",
                //"Temperature": 0.5,
                //"MaxTokens": 16000,
                //"JsonCapability": "JsonSchema"
                "BaseUrl": "https://integrate.api.nvidia.com/v1",
                "ApiKey": "<key>",
                "Model": "meta/llama-3.1-8b-instruct",
                "Temperature": 0.5,
                "MaxTokens": 16000,
                "JsonCapability": "None"
              },
                "Outliner": {
                    "Provider": "OpenRouter",
                    //"BaseUrl": "https://generativelanguage.googleapis.com/v1beta/openai/",
                    //"ApiKey": "<key>",
                    //"Model": "gemini-2.5-flash",
                    //"Temperature": 0.5,
                    //"MaxTokens": 16000,
                    //"JsonCapability": "JsonSchemat"
                     "BaseUrl": "https://openrouter.ai/api/v1",
                     "ApiKey": "<key>",
                     "Model": "openai/gpt-oss-120b:free",
                     "Temperature": 0.7,
                     "MaxTokens": 16000,
                     "JsonCapability": "JsonObject"
                },
                "Writers": {
                    "Provider": "NVidea",
                    "BaseUrl": "https://integrate.api.nvidia.com/v1",
                    "ApiKey": "<key>",
                    "Model": "meta/llama-3.1-8b-instruct",
                    "Temperature": 0.5,
                    "MaxTokens": 16000,
                    "JsonCapability": "None"
                },
                "Critic": {
                    "Provider": "Groq",
                    "BaseUrl": "https://api.groq.com/openai/v1",
                    "ApiKey": "<key>",
                    "Model": "openai/gpt-oss-20b",
                    "Temperature": 0.7,
                    "MaxTokens": 16000,
                    "JsonCapability": "None"
                },
                "Rewriter": {
                    "Provider": "AILM",
                    "BaseUrl": "https://api.aimlapi.com/v1",
                    "ApiKey": "<key>",
                    "Model": "qwen3-32b",
                    "Temperature": 0.5,
                    "MaxTokens": 16000,
                    "JsonCapability": "None"
                },
                "Publisher": {
                    "Provider": "Groq",
                    "BaseUrl": "https://api.groq.com/openai/v1",
                    "ApiKey": "<key>",
                    "Model": "openai/gpt-oss-20b",
                    "Temperature": 1.0,
                    "MaxTokens": 16000,
                    "JsonCapability": "JsonObject"
                },
                "StoryReviewer": {
                    "Provider": "Groq",
                    "BaseUrl": "https://api.groq.com/openai/v1",
                    "ApiKey": "<key>",
                    "Model": "openai/gpt-oss-20b",
                    "Temperature": 0.5,
                    "MaxTokens": 16000,
                    "JsonCapability": "None"
                },
                "QuestionsAssistant": {
                    "Provider": "AILM",
                    "BaseUrl": "https://api.aimlapi.com/v1",
                    "ApiKey": "<key>",
                    "Model": "qwen3-32b",
                    "Temperature": 0.5,
                    "MaxTokens": 16000,
                    "JsonCapability": "None"
                }
            },
            "DefaultProvider": "IntentionAnalyzer"
        },
        "StepTypeStyles": {
            "BaseLlmStep": {
                "Icon": "MessageSquare",
                "Color": "#3B82F6",
                "BorderColor": "#60A5FA",
                "BackgroundColor": "#EFF6FF"
            },
            "IntentionAnalyzerStep": {
                "Icon": "BrainCircuit",
                "Color": "#8B5CF6",
                "BorderColor": "#A78BFA",
                "BackgroundColor": "#F5F3FF"
            },
            "IntentionRouterStep": {
                "Icon": "GitFork",
                "Color": "#F59E0B",
                "BorderColor": "#FBBF24",
                "BackgroundColor": "#FEF3C7"
            },
            "GroupStep": {
                "Icon": "Box",
                "Color": "#64748B",
                "BorderColor": "#94A3B8",
                "BackgroundColor": "#F8FAFC"
            },
            "ParallelStep": {
                "Icon": "Columns",
                "Color": "#10B981",
                "BorderColor": "#34D399",
                "BackgroundColor": "#ECFDF5"
            },
            "StatelessRewriterStep": {
                "Icon": "PenTool",
                "Color": "#EC4899",
                "BorderColor": "#F472B6",
                "BackgroundColor": "#FDF2F8"
            },
            "DelegatedStep": {
                "Icon": "Code",
                "Color": "#6B7280",
                "BorderColor": "#9CA3AF",
                "BackgroundColor": "#F9FAFB"
            }
        }
    }
}
